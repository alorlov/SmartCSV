{
  "configSchema": {
    "db": {
      "type": "object",
      "properties": {
        "host": {
          "type": "string",
          "default": "10.44.116.9"
        },
        "user": {
          "type": "string",
          "default": "root"
        },
        "password": {
          "type": "string",
          "default": ""
        },
        "database": {
          "type": "string",
          "default": "eliteconnect"
        }
      }
    },
    "projectPath": {
      "type": "string",
      "default": "C:/elite",
      "title": "Project directory"
    },
    "showOnRightSide": {
      "type": "boolean",
      "default": true
    }
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "tablr-models": {
        "versions": {
          "1.0.0": "consumeTablrModelsServiceV1"
        }
      }
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.0",
    "fs-plus": "^2.3.0",
    "pathwatcher": "*",
    "q": "^1.0.1",
    "underscore-plus": "^1.0.0",
    "underscore.string": "*",
    "xml-to-js": "*"
  },
  "devDependencies": {},
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "homepage": "https://github.com/alorlov/smartcsv#readme",
  "license": "MIT",
  "main": "./lib/smartcsv",
  "name": "smartcsv",
  "description": "Framework for UI automation language",
  "optionalDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alorlov/smartcsv.git"
  },
  "version": "1.0.0"

}
